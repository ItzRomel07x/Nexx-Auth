{
  "version": 2,
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/[...auth]"
    },
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/v1/[...api]"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  }
}