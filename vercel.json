{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/auth/(.*)",
      "destination": "/api/auth/[...auth]"
    },
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/v1/[...api]"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.15"
    }
  }
}
